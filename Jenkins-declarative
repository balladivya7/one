pipeline {
    agent any

    stages {
        stage('Git Checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/piyushmj12/golden_image_packer']]])
            }
        }
        stage("Running for packer template"){
            steps {
               
               echo"Running Packer template"
            }
        }
    }
}
